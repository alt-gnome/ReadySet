<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="ready-set">

	<schema id="org.altlinux.ReadySet" path="/space/rirusha/ReadySet/">

		<key name="window-width" type="i">
			<default>600</default>
		</key>
		<key name="window-height" type="i">
			<default>500</default>
		</key>
		<key name="window-maximized" type="b">
			<default>false</default>
		</key>
		<key name="show-steps" type="b">
			<default>false</default>
		</key>

		<key name="all-steps" type="as">
			<default>[
				'language',
				'keyboard',
				'step-4',
				'step-5',
				'step-6',
				'test',
				'step-8'
			]</default>
			<summary>Default Order.</summary>
			<description>Default Order</description>
		</key>

	</schema>

</schemalist>
